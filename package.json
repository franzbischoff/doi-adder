{
  "name": "doi-adder",
  "displayName": "Doi Adder",
  "description": "This package allows you to quickly add bibtex references to a bib file from the DOI.",
  "publisher": "zccajgb",
  "repository": "https://github.com/zccajgb/doi-adder",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:doi-adder.doiAdd"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "doi-adder.doiAdd",
        "title": "DOI Add"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jquery": "^3.6.0",
    "jsdom": "^17.0.0",
    "valid-url": "^1.0.9"
  }
}
